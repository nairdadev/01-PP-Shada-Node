	<header>
			<!-- TOP HEADER -->
			<div id="top-header">
				<div class="container">
					<ul class="header-links pull-left">
						<li><a href="#"><i class="fa fa-phone"></i> +021-95-51-84</a></li>
						<li><a href="#"><i class="fa fa-envelope-o"></i> email@email.com</a></li>
						<li><a href="#"><i class="fa fa-map-marker"></i> dirección</a></li>
					</ul>
					<ul class="header-links pull-right">
                    {{# if login }}
          	<li><a href="#"><i class="fa fa-dollar"></i> Pesos Argentinos</a></li>
						<li><a href="/user/profile"><i class="fa fa-user-o"></i> Mi Cuenta</a></li>
          {{ else }}
            <a class="dropdown-item" href="/user/signup">Registrarse</a>
            <a class="dropdown-item" href="/user/signin">Ingresar</a>   
          {{/if}}
					
					
          </ul>
				</div>
			</div>
			<!-- /TOP HEADER -->

			<!-- MAIN HEADER -->
			<div id="header">
				<!-- container -->
				<div class="container">
					<!-- row -->
					<div class="row">
						<!-- LOGO -->
						<div class="col-md-3">
							<div class="header-logo">
								<a href="#" class="logo">
									<img src="./img/logo_grande.png" style="width:250px"alt="">
								</a>
							</div>
						</div>
						<!-- /LOGO -->

						<!-- SEARCH BAR -->
						<div class="col-md-6">
							<div class="header-search">
								<form>
									<select class="input-select">
                    <option value="0">Categorías</option>
                    {{#each category}}
										<option value="1">{{this.name}}</option>
										{{/each}}
									</select>
									<input class="input" placeholder="Buscar">
									<button class="search-btn">Buscar</button>
								</form>
							</div>
						</div>
						<!-- /SEARCH BAR -->

						<!-- ACCOUNT -->
						<div class="col-md-3 clearfix">
							<div class="header-ctn">
							 

													<!-- Cart -->
								<div class="dropdown">
									<a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
										<i class="fa fa-shopping-cart"></i>
										<span>Carrito</span>
										<div class="qty">{{ session.cart.totalQty }}</div>
									</a>
									<div class="cart-dropdown">
										<div class="cart-list">
                      {{#each session.cart.items }}
											<div class="product-widget">
												<div class="product-img">
													<img src="{{this.item.imagePath}}" alt="">
												</div>
												<div class="product-body">
													<h3 class="product-name"><a href="#">{{ this.item.title }}</a></h3>
													<h4 class="product-price"><span class="qty">{{this.qty}}x</span>${{ this.item.price }}</h4>
												</div>
                        <a class="delete" href="/remove/{{ this.item._id }}"><i class="fa fa-close"></i></a>
											</div>
                      {{/each}}
										</div>
										<div class="cart-summary">
											<small>{{session.cart.totalQty}} Item(s) seleccionados</small>
											<h5>SUBTOTAL: ${{session.cart.totalPrice}}</h5>
										</div>
										<div class="cart-btns">
											<a href="/shopping-cart">Ver Carrito</a>
											<a href="/checkout">Checkout  <i class="fa fa-arrow-circle-right"></i></a>
										</div>
									</div>
								</div>
								<!-- /Cart -->

								<!-- Menu Toogle -->
								<div class="menu-toggle">
									<a href="#">
										<i class="fa fa-bars"></i>
										<span>Menu</span>
									</a>
								</div>
								<!-- /Menu Toogle -->
							</div>
						</div>
						<!-- /ACCOUNT -->
					</div>
					<!-- row -->
				</div>
				<!-- container -->
			</div>
			<!-- /MAIN HEADER -->
		</header>
		<!-- /HEADER -->

		<!-- NAVIGATION -->
		<nav id="navigation">
			<!-- container -->
			<div class="container">
				<!-- responsive-nav -->
				<div id="responsive-nav">
					<!-- NAV -->
					<ul class="main-nav nav navbar-nav">
						<li class="active"><a href="#">Home</a></li>
						<li><a href="#">Hot Deals</a></li>
						<li><a href="#">Categories</a></li>
						<li><a href="#">Laptops</a></li>
						<li><a href="#">Smartphones</a></li>
						<li><a href="#">Cameras</a></li>
						<li><a href="#">Accessories</a></li>
					</ul>
					<!-- /NAV -->
				</div>
				<!-- /responsive-nav -->
			</div>
			<!-- /container -->
		</nav>
		<!-- /NAVIGATION -->
